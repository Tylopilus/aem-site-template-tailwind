{
    "name": "aem-site-template-standard-theme",
    "version": "0.0.1",
    "description": "Standard AEM site theme",
    "source": "src/main.ts",
    "main": "dist/theme.js",
    "license": "MIT License, Copyright 2020 Adobe Systems Incorporated",
    "scripts": {
        "build": "parcel build",
        "watch": "parcel watch",
        "proxy": "aem-site-theme-builder live",
        "live": "npm-run-all -p watch proxy"
    },
    "devDependencies": {
        "@adobe/aem-site-theme-builder": "5.2.0",
        "autoprefixer": "^10.3.7",
        "normalize.css": "^8.0.1",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.0.1",
        "@parcel/transformer-sass": "^2.0.1",
        "postcss": "^8.3.11"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {
                "overrideBrowserslist": [
                    "> 1%",
                    "last 2 versions"
                ]
            }
        }
    }
}
